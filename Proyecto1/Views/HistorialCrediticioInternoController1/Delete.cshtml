    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Confirmar el Borrado</h1>
<forn method="post" asp-action= "Delete">
    <input asp-for="Id" Type="hidden"/>
    <label>Puntaje</label>
    <input asp-for="Puntaje" />
    <br/>
    <label>Deudas Activas</label>
    <input asp-for="DeudasActivas" />
